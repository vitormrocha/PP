Ex 10: #include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
    float alt, peso;
    char sex;
    printf ("Apresente a sua altura: ");
    scanf ("%f", &alt);
    printf ("Apresente o seu sexo. Digite F para feminino ou M para masculino: ");
    sex= getchar ();
    scanf ("%c", &sex);

    if (sex == 'F') {
        peso= (62.1*alt)-44.7;
        printf("\nO seu peso ideal eh: %.1f", peso);
    }
    if (sex == 'M') {
        peso= (72.1*alt)-58.0;
        printf("\nO seu peso ideal eh: %.1f", peso);
    }
    return 0;
}

