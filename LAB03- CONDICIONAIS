Ex 24: #include <stdio.h>
#include <stdlib.h>
#include <math.h>


int main () {
   int imp, num, mg, sp, rj, ms;
   printf ("Apresente um valor de um produto: ");
   scanf ("%d", &imp);
   printf ("Digite um numero entre 1 e 4 para escolher a destinacao do produto.");
   printf("\n 1: Minas Gerais (MG) \n 2: Sao Paulo (SP) \n 3: Rio De Janeiro (RJ) \n 4: Mato Grosso do Sul (MS)\n");
   scanf("%d", &num);

   switch ( num ) {
   case 1:
    mg= (imp * 0.07) + imp;
    printf ("\nMG selecionado. O valor final do produto com a taxa de imposto eh: %d", mg); break;

   case 2:
    sp= (imp*0.12) + imp;
    printf ("\nSP selecionado. O valor final do produto com a taxa de imposto eh: %d", sp); break;

   case 3:
    rj= (imp*0.15) + imp;
    printf("\nRJ selecionado. O valor final do produto com a taxa de imposto eh: %d", rj); break;

   case 4:
    ms= (imp * 0.08) + imp;
    printf("\nMS selecionado. O valor final do produto com a taxa de imposto eh: %d", ms); break;

   }

    return 0;
}

Ex 25: #include <stdio.h>
#include <stdlib.h>
#include <math.h>


int main () {
       float a, b, c, delta, x1, x2;
       printf ("Vamos calcular uma equacao do segundo grau. para ax^2+bx+c=0.");
       printf("\na= ");
       scanf ("%f", &a);
       printf("\nb= ");
       scanf ("%f", &b);
       printf("\nc= ");
       scanf ("%f", &c);

       delta= (b*b)-4*(a*c);

       if (delta<0){
        printf("Nao existe raiz.");
       }
       if (delta>0) {
        x1= (-b -sqrt(delta))/2* a;
        x2= (-b +sqrt(delta))/2 * a;
        printf("As raizes sao: %.1f e %.1f", x1, x2);
       } if (delta == 0){
       x1= -b/2*a;
       printf("Raiz unica, que equivale a: %.1f", x1);
       }

    return 0;
}

Ex 26: #include <stdio.h>
#include <stdlib.h>
#include <math.h>


int main () {
       float gasolina, km, consum;
       printf("Apresente a distancia em quilometros rodados e a quantidsde de gasolina consumidos em litros: ");
       scanf("%f %f", &km, &gasolina);
       consum= km/gasolina;

       if (consum<=8) {
        printf ("Venda o carro!");
       } if (consum >8 && consum<14) {
       printf("Economico!");
       } if (consum>=14) {
       printf ("Super Economico!");
       }

    return 0;
}

