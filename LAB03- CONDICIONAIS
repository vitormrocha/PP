{
    int num1, num2, dif, dif2;
        printf("Digite dois numeros: ");
        scanf ("%d %d", &num1, &num2);


        if (num1>num2) {
            printf (" O numero maior eh: %d \n", num1);
            dif= num1 - num2;
            printf("A diferenca entre esses numeros eh: %d", dif);

        }
        else {

        printf("O numero maior eh: %d \n", num2);
        dif2= num2-num1;
        printf ("A diferenca entre esses numeros eh: %d", dif2);
        }


    return 0;
}

