    scanf ("%d", &dia);
    printf ("Digite o mes: ");
    scanf ("%d", &mes);
    printf("\nDigite o ano: ");
    scanf("%d", &ano);

    if (mes<1 || mes>12) {
        printf ("\nMes invalido.");
    } else if (dia <1 || dia > 12) {
        printf ("\nDia invalido.");
    } else if ( mes == 4 || mes == 6 || mes == 9 || mes == 11) {
        if (dia> 30) {
            printf ("\nData invalida.");
        }  else {
            printf ("\n Data valida.");
        }
    } else if (mes== 2) {
    if (ano%400==0 && ano%100!=0 ) {
        if (dia > 29) {
            printf ("\n Data invalida. ");
        } else {
        printf ("\nData valida.");
        } }else {
            if (dia > 28) {
                printf ("\n Data invalida.");
            } else {
                printf ("\nData Valida.");
            }
        } }else {
            printf ("\nData valida.");
        }
        return 0;
    }

