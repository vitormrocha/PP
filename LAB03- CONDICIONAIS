Ex 17:#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
   float basema, basem, alt, trape;
   printf("Apresente o valor da base maior de um trapezio: ");
   scanf ("%f", &basema);
   printf("\nApresente o valor da base menor de um trapezio:");
   scanf ("%f", &basem);
   printf ("\nApresente o valor da altura desse trapezio: ");
   scanf ("%f", &alt);
   trape= ((basema + basem)*alt)/2;

   printf ("\nO valor da area desse trapezio eh: %.1f", trape);

   return 0;
}


Ex 18:
#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
   int num, num1, num2, add,sub,multi,divi;
   printf ("Digite um numero entre 1 e 4 para escolher uma operacao a ser realizada. ");
   printf ("\n1: Adicao \n2: Subtracao \n3: Multiplicacao \n4: Divisao. \n");
   scanf("%d", &num);
   switch( num ){
   case 1:
       printf ("Adicao selecionada. Selecione dois numeros: ");
       scanf ("%d %d", &num1, &num2);
       add= num1 + num2;
       printf ("O resultado eh: %d", add); break;
   case 2:
       printf ("Subtracao selecionada. Selecione dois numeros: ");
       scanf ("%d %d", &num1, &num2);
       sub= num1 - num2;
       printf("O resultado eh: %d", sub); break;
   case 3:
       printf ("Multiplicacao selecionada. Selecione dois numeros: ");
       scanf ("%d %d", &num1, &num2);
       multi= num1 * num2;
       printf("O resultado eh: %d", multi); break;
   case 4:
    printf ("Divisao selecionada. Selecione dois numeros: ");
    scanf ("%d %d", &num1, &num2);
    if (num2 == 0) {
    printf ("Operacao invalida."); break;
    } else {
	divi= num1/num2;
        printf ("O resultado eh: %d", divi);
   } break;
   default: printf ("Numero invalido."); break;
}
    return 0;
}

Ex 19: #include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
   int num, num3, num5;
   printf ("Apresente um numero inteiro: ");
   scanf ("%d", &num);
   num3= num%3;
   num5= num%5;

   if (num3 == 0) {
    printf ("\nO numero e divisivel por 3.");
   } else {
   printf ("\nO numero nao e divisvel por 3.");
   }
   if (num5 == 0) {
    printf ("\nO numero e divisel por 5.");
   }else {
   printf ("\nO numero nao e divisivel por 5");
   }
   if (num3 == 0 && num5==0) {
    printf ("\nO numero e divisel por 3 e 5");
   } else {
   printf ("\nO numero nao e divisel por 3 e 5");
   }
   return 0;
}

Ex 20: #include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
   int val, val2, val3;
   printf("Digite os valores de tres lados de um triangulo: ");
   scanf ("%d %d %d", &val, &val2, &val3);

   if (val == val2 && val2 == val3 && val == val3) {
    printf("\nEsse triangulo e equilatero. ");

   } if (val!=val2 && val2!=val3 && val!=val3) {
   printf ("\nEsse triangulo eh escaleno.");
   }
   if (val == val2 && val2!= val3 && val!= val3) {

    printf ("\nEsse trianglo e isosceles.");
   }
   return 0;
}

Ex 21:  #include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
   int val, num, num2, add,sub,multi,divi;
   printf ("Digite um numero entre 1 e 4 para escolher uma operacao a ser realizada. ");
   printf ("\n1: Adicao \n2: Subtracao \n3: Multiplicacao \n4: Divisao. \n");
   scanf("%d", &val);
   switch( val ){
   case 1:
       printf ("Adicao selecionada. Selecione dois numeros: ");
       scanf ("%d %d", &num, &num2);
       add= num + num2;
       printf ("O resultado eh: %d", add); break;
   case 2:
       printf ("Subtracao selecionada. Selecione dois numeros: ");
       scanf ("%d %d", &num, &num2);
       sub= num - num2;
       printf("O resultado eh: %d", sub); break;
   case 3:
       printf ("Multiplicacao selecionada. Selecione dois numeros: ");
       scanf ("%d %d", &num, &num2);
       multi= num * num2;
       printf("O resultado eh: %d", multi); break;
   case 4:
    printf ("Divisao selecionada. Selecione dois numeros: ");
    scanf ("%d %d", &num, &num2);
    if (num2 == 0) {
    printf ("A operacao nao eh valida."); break;
    } else {
        divi= num/num2;
        printf ("O resultado eh: %d", divi);
   } break;
   default: printf ("Numero invalido."); break;
}
    return 0;
}
