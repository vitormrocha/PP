int main () {
   int numsem;
   printf ("Digite um numero inteiro entre 1 e 7: ");
   scanf ("%d", &numsem);
   switch( numsem ){
   case 1 : printf ("O dia da semana correspondente eh: Domingo"); break;
   case 2 : printf ("O dia da semana correspondente eh: Segunda"); break;
   case 3 : printf ("O dia da semana correspondente eh: Terca"); break;
   case 4: printf ("O dia da semana correspondente eh: Quarta"); break;
   case 5: printf ("O dia da semana correspondente eh: Quinta"); break;
   case 6: printf ("O dia da semana correspondente eh: Sexta"); break;
   case 7: printf ("O dia da semana correspondente eh: Sabado"); break;
   default: printf ("Numero invalido."); break;

   }
   return 0;
}

