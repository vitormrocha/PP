   printf ("Apresente um numero inteiro: \n");
   scanf ("%d", &num);

   if (num>10) {
    num1= num%10;
    num= num/10;
    num2= num%10;
    num=num/10;
    soma= num1 + num2;
    printf ("A soma dos algarismos eh: %d", soma);


}
    if (num <= 0) {
        printf(" Numero invalido.");
    } if (num>100){
        num1= num%10;
    num= num/10;
    num2= num%10;
    num=num/10;
    num3= num%10;
    num=num/10;
    soma2= num1 + num2 + num3;
    printf ("A soma dos algarismos eh: %d", soma2);
    }


    return 0;
}

