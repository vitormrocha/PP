    printf ("\nApresente o ano do seu aniversario: ");
    scanf ("%d", &ano);


   if (dia > 31) {
        printf ("Data invalida.");
    } if (mes == 2 && dia > 28) {
        printf("\nData invalida!");
    } if (mes == 2 && ano%400==0 || ano%4==0 && ano%100 !=0 && dia>29) {
        printf ("Data invalida!");
