Ex 28: #include <stdio.h>
#include <stdlib.h>
#include <math.h>


int main () {
           int num, x, y, z, geo,pond, harm, arit;
           printf ("Digite tres valores: ");
           scanf ("%d %d %d", &x, &y, &z);
           printf ("Digite um numero correspondente a operacao que se deseja realizar. \n1: Media Geometrica. \n2: Media Ponderada. \n3: Media Harmonica. \n4: Media Aritmetica.");
           scanf ("%d", &num);
           switch ( num ) {
           case 1:
               geo= cbrt((x*y*z));
               printf ("A media geometrica eh: %d", geo); break;
           case 2:
                pond=((1*x)+ (2*y)+(3*z))/3;
                printf("A media ponderada eh: %d", pond); break;
           case 3:
                harm= 1/(1/x)+(1/y)+(1+z);
                printf("A media harmonica eh: %d", harm); break;
           case 4:
                arit= (x+y+z)/3;
                printf("A media aritmetica eh: %d", arit); break;

           default: printf("Numero invalido."); break;
           }

       return 0;
}

Ex 29: #include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
    int resp, acerto;
    acerto=0;

    printf ("Prova de matematica: \n1: Quanto e 2 + 2? ");
    scanf ("%d", &resp);
    if (resp != 4) {
        printf ("\nResposta errada.");
    } else {
        printf ("\nResposta certa.");
        acerto++;
    } +
    printf ("\n2: Quanto eh 10 + 3? ");
    scanf ("%d", &resp);

    if (resp != 13) {
        printf ("\nResposta errada.");
    } else {
        printf ("\nResposta certa.");
        acerto++;
    }
    printf ("\n3: Quanto eh 49 + 37?");
    scanf ("%d", &resp );

    if (resp != 86) {
        printf ("\nResposta errada.");
    } else {
        printf ("\nResposta certa.");
        acerto++;
    }
    printf ("\n4: Quanto eh: 50 + 50?");
    scanf ("%d", &resp);

    if (resp != 100) {
        printf ("\nResposta errada.");
    } else {
        printf ("\nResposta certa.");
        acerto++;
    }
    printf ("\n5: Quanto eh 56 + 24?");
    scanf ("%d", &resp);

    if (resp!= 80) {
        printf ("\nResposta errada.");
    } else {
        printf ("\nResposta certa.");
        acerto++;
    }
    printf ("\n\nProva finalizada. Quantidade de acertos: %d", acerto);


        return 0;

}



Ex 30: #include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int numa, numb, numc;
    printf ("Apresente tres numeros: ");
    scanf ("%d %d %d", &numa, &numb, &numc);

    if (numa > numb && numb > numc) {
        printf ("A ordem crescente eh: %d, %d, %d", numc, numb, numa);
    } if (numb>numa && numa>numc) {
        printf ("A ordem crescente eh: %d %d %d", numc, numa, numb);
    } if (numc>numa && numa > numb){
        printf ("A ordem crescente eh: %d, %d, %d", numb, numa, numc);
    } if (numc>numb && numb>numa) {
        printf ("A ordem crescente eh: %d %d %d", numa, numb, numc);
    }
     if (numa>numc && numc >numb) {
        printf ("A ordem crescente eh: %d, %d %d", numb, numc,numa);
    } if (numb>numc && numc>numa) {
        printf ("A ordem crescente eh: %d %d %d", numa, numc, numb);
    }
    return 0;
}

Ex 31: #include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main() {
    float alt, peso;
    printf ("Apresente a sua altura e peso: ");
    scanf ("%f %f",&alt, &peso);

    if (alt <= 1.20 && peso <=60) {
        printf ("Classificacao: A");
    } if (alt <= 1.20 && peso > 60 && peso <=90){
        printf ("Classificacao: D");
    } if (alt<=1.20 && peso > 90) {
        printf ("Classificacao: G");
    } if (alt > 1.20 && alt <=1.70 && peso <= 60 ) {
        printf ("Classificacao: B");
    } if (alt > 1.20 && alt <=1.70 && peso > 60 && peso <=90 ) {
        printf ("Classificacao: E");
    } if (alt > 1.20 && alt <=1.70 && peso >90) {
        printf ("Classificacao: H");
    } if (alt>1.70 && peso<=60) {
        printf("Classificacao: C");
    } if (alt> 1.70 && peso > 60 && peso <=90) {
        printf ("Classificacao: F");
    } if (alt>1.70 && peso > 90 ){
        printf ("Classificacao: I");
    }
    return 0;
}

Ex 32: #include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int num, quant;
    float cach, baus, bauo, hamb, cheese, suco, refri;
    printf("Digite um codigo de 100 a 106 para escolher o seu item.\n100: cachorro quente\n101:Bauru Simples\n102:Bauru com Ovo\n103:Hamburger\n104:Cheeseburger\n105:Suco\n106:refri");
    scanf ("%d", &num);
    switch ( num ){
    case 100: printf ("Escolha a quantidade: ");
    scanf ("%d", &quant);
    cach= quant * 1.20; printf ("O valor a ser pago eh: %.2f", cach); break;
    case 101: printf ("Escolha a quantidade: ");
    scanf ("%d", &quant);
    baus= quant * 1.30; printf("O valor a ser pago eh: %.2f", baus); break;
    case 102: printf ("Escolha a quantidade: ");
    scanf ("%d", &quant);
    bauo= quant * 1.50; printf("O valor a ser pago eh: %.2f", bauo); break;
    case 103: printf ("Escolha a quantidade: ");
    scanf ("%d", &quant);
    hamb= quant * 1.20; printf ("O valor a ser pago eh: %.2f", hamb); break;
    case 104: printf ("Escolha a quantidade: ");
    scanf ("%d", &quant);
    cheese= quant * 1.70; printf("O valor a ser pago eh: %.2f", cheese); break;
    case 105: printf ("Escolha a quantidade: ");
    scanf ("%d", &quant);
    suco= quant * 2.20; printf ("O valor a ser pago eh: %.2f", suco); break;
    case 106: printf ("Escolha a quantidade: ");
    scanf ("%d", &quant);
    refri= quant * 1.00; printf("O valor a ser pago eh: %.2f", refri); break;
    default: printf("Numero invalido."); break;
    }
    return 0;
}

Ex 33: #include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
    float precoant, preconov, percent;
    printf ("Apresente u preco de um produto; ");
    scanf ("%f", &precoant);

    if (precoant <= 50) {
        percent= 0.05;
        preconov= precoant * (1 + percent);
        printf ("O preco novo eh: %.2f", preconov);

    } if (precoant >= 50 && precoant<=100) {
        percent= 0.1;
         preconov= precoant * (1 + percent);
        printf ("O preco novo eh: %.2f", preconov);

    } if (precoant > 100) {
        percent= 0.15;
         preconov= precoant * (1 + percent);
        printf ("O preco novo eh: %.2f", preconov);
    }
    if (preconov <=80 ) {
        printf ("\nBarato!");
    } else if (preconov > 80 && preconov <= 120) {
        printf ("\nNormal.");
    } else if (preconov > 120 && preconov <= 200) {
        printf ("\nCaro.");
    } else {
        printf ("M\nMuito caro!.");
    }


    return 0;
    }

