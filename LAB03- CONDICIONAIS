Ex 1: #include <stdio.h>
#include <stdlib.h>

int main()
{
    int num1, num2;
    printf("Digite dois numeros: ");
    scanf ("%d %d", &num1, &num2 );
    if (num1> num2){
        printf("O primeiro numero e maior");

    }
    else {
        printf("O segundo numero e maior");
    }

    return 0;
}

Ex 2: #include <stdio.h>
#include <stdlib.h>
#include <math.h>
int main()
{
    float num1;
    printf ("Digite um numero: ");
    scanf ("%f", &num1);

    if (num1>= 0){
            num1= sqrt(num1);
            printf ("A raiz quadrada desse numero eh: %.1f", num1);
    }
    else {
        printf("O numero e invalido.");
    }
    return 0;
}

Ex 3: #include <stdio.h>
#include <stdlib.h>
#include <math.h>
int main()
{
    float num1;
    printf ("Digite um numero: ");
    scanf ("%f", &num1);

    if (num1>= 0){
            num1= sqrt(num1);
            printf ("A raiz quadrada desse numero eh: %.1f", num1);
    }
    else {
        num1= num1*num1;
        printf("A raiz quadrada desse numero eh: %.1f", num1);
    }
    return 0;
}

Ex 4: #include <stdio.h>
#include <stdlib.h>
#include <math.h>
int main()
{
    float num1, raiz, pot;
    printf ("Digite um numero: ");
    scanf ("%f", &num1);

    if (num1>0){
            pot= num1*num1;
            raiz= sqrt(num1);
            printf ("A potencia desse numero ao quadrado eh: %.1f \n", pot);
            printf("A raiz desse numero eh: %.1f", raiz);
    }
    else {
            printf("O numero e invalido.");
    }
    return 0;
}

