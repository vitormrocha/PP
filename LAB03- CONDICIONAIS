Ex 13: #include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
   float not1, not2, not3, mediap;
   printf ("Apresente a nota de um aluno em tres provas: ");
   scanf ("%f %f %f", &not1, &not2, &not3);
   mediap= (not1*1.0) + (not2*1.0) + (not3 * 2.0)/(1.0 + 1.0 + 2.0);

   if (mediap>= 60) {
    printf ("\nA media eh: %.1f, aluno aprovado!", mediap);
   } else {
   printf ("\n,A media eh: %.1f, aluno nao aprovado.", mediap);
   }

   return 0;
}

Ex 14: #include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
   float lab, avals, exam, mediap;
   printf ("Apresente as notas de um aluno na atividade do laboratorio, no exame semestral, e no exame semestral : ");
   scanf ("%f %f %f", &lab, &avals, &exam);
   mediap= (lab*2.0) + (avals*3.0) + (exam * 5.0)/10;

   if (mediap<= 2.9 && mediap>=0) {
    printf ("\nA media eh: %.1f, aluno reprovado.", mediap);
   } if (mediap >= 3.0 && mediap <= 4.9) {
       printf ("\nA media eh: %.1f, aluno de recuperacao. ", mediap);

   } if (mediap >= 5.0) {
        printf ("\n A media eh: %.1f, aluno aprovado!", mediap);
   }

   return 0;
}

