Ex 39: #include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
    float sal, temp, salfin, bonus;
    printf ("Apresente o valor do salario de um funcioario: ");
    scanf ("%f", &sal);
    printf ("\nApresente o tempo de servico desse funcionario em anos: ");
    scanf ("%f", &temp);

    if (sal <= 500.00 && temp <= 1) {
        bonus= 0;
        salfin= sal * 1.25 + bonus;
        printf ("\nO salario desse funcionario com o aumento sera: %.2f", salfin);
    } else if (sal <= 1000.00 && temp <= 3) {
        bonus= 100;
        salfin= sal * 1.20 + bonus;
        printf ("\nO salario desse funcionario com o aumento sera: %.2f", salfin);
    } else if (sal <= 1500.00 && temp <= 6) {
        bonus= 200;
        salfin= sal * 1.15 + bonus;
        printf("\nO salario desse funcionario com o aumento sera:  %.2f", salfin);
    } else if (sal <= 2000.00 && temp <= 10 ) {
        bonus= 300;
        salfin= sal * 1.10 + bonus;
        printf ("\nO salario desse funcionario com o aumento sera: %.2f", salfin);
    } else if (sal > 2000.00 && temp > 10 ) {
        bonus= 500;
        salfin= sal + bonus;
        printf ("\nO salario desse funcionario com o aumento sera: %.2f", salfin);

    }

    return 0;
}

Ex 40: #include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
    float custofabrica,comissao,imposto,custoconsum;
    printf("Apresente o custo da fabrica: ");
    scanf ("%f", &custofabrica);

    if (custofabrica<=12000) {
        comissao= 0.05;
        imposto= 0;
    } else if (custofabrica <= 25000) {
        comissao= 0.10;
        imposto= 0.15;
    } else if (custofabrica > 25000) {
        comissao= 0.15;
        imposto= 0.20;
    }
    custoconsum= custofabrica * (1+comissao+imposto);
    printf ("O custo ao consumidor eh: %.2f", custoconsum);

    return 0;
}
 
