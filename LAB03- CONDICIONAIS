    if (venda >= 100000) {
        comiss= 700 + venda * 0.16;
        printf ("\nO preco da comissao eh: %.2f", comiss);
    } else if (venda < 100000 && venda >= 80000) {
        comiss= 650 + venda * 0.14;
        printf ("\nO preco da comissao eh: %.2f", comiss);
    } else if (venda < 80000 && venda >= 60000) {
        comiss= 600 + venda * 0.14;
        printf ("\nO preco da comissao eh: %.2f", comiss);
    } else if (venda < 60000 && venda >= 40000) {
        comiss= 550 + venda * 0.14;
        printf ("\nO preco da comissao eh: %.2f", comiss);
    } else if (venda > 40000 && venda >= 20000) {
        comiss= 500 + venda * 0.14;
        printf ("\nO preco da comissao eh: %.2f", comiss);
    } else if (venda < 20000) {
        comiss= 400 + venda * 0.14;
        printf ("\nO preco da comissao eh: %.2f", comiss);
    }

    return 0;
    }

Ex 37: 
int main () {
    int valor, hor, min, hor2, min2, horpart, minpart, tarifa;

    printf ("Apresente a hora da chegada: ");
    scanf ("%d", &hor);
    printf ("\nApresente os minutos na hora da chegada: ");
    scanf ("%d", &min);
    printf ("\nApresente a hora da partida: ");
    scanf ("%d", &hor2);
    printf ("\nApresente os minutos na hora da partida: ");
    scanf ("%d", &min2);
    horpart= hor2-hor;
    minpart= min2-min;

    if (horpart <= 2 && minpart < 60 ) {
            printf ("\nA tarifa a ser paga eh: R$1.00 ");
    } else if (minpart >= 61 ) {
        printf ("\n A tarifa a ser paga eh: R$1.40");
    } else if (horpart > 2 && horpart <= 4 && minpart <= 60) {
        printf ("\nA tarifa a ser paga eh: R$1.40");
    } else if (minpart >= 61) {
        printf ("\nA tarifa a ser paga eh: R$2.0");
    } else if (horpart > 4 && horpart >= 5 && minpart <= 60) {
        tarifa= (horpart - 5) + 2.00;
        printf ("\nA tarifa a ser paga eh: R$%d", tarifa);
    }



    return 0;
}

