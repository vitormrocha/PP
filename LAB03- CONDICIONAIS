Ex 8: #include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main () {
    float not1, not2, med;
    printf("Apresente a nota 1: ");
    scanf("%f", &not1);
    med= not1+not2/2;

    if (not1 >0 && not1<=10) {

        printf("A nota 1 eh valida \n");

    }
    if (not1<0) {

        printf ("A nota 1 nao e valida");
    }
    if (not1>10) {

        printf ("A nota 1 nao e valida");

    }
    printf ("\nApresente a segunda nota: ");
    scanf ("%f", &not2);

    if (not2>0 && not2<=10) {
        printf("A nota 2 eh valida: \n");
        printf("A media dessas duas notas eh: %.1f", med);
    }
    if (not2<0) {
        printf ("A nota 2 nao eh valida");
    }
    if (not2>10) {
        printf ("A nota 2 nao eh valida");
    }


    return 0;

}
