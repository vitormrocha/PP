Ex 31: #include <stdio.h>
#include <math.h>

int main() {
    int num, ant, suc;
    printf(" Apresente um numero inteiro: ");
    scanf("%D", &num);
    ant= num-1;
    suc=num+1;
    printf(" O antecessor desse numero e: %d \n", ant);
    printf("O sucessor desse numero e: %d \n", suc);

        return 0;

}

Ex 32: #include <stdio.h>
#include <math.h>

int main() {
    int num, ant, suc, soma;
    printf(" Apresente um numero inteiro: ");
    scanf("%D", &num);
    ant=(num * 2)- 1;
    suc= (num * 3) + 1;
    soma= ant + suc;
    printf(" O antecessor desse numero e: %d \n", ant);
    printf("O sucessor desse numero e: %d \n", suc);
    printf("A soma desses valores e: %d", soma);

        return 0;

}

