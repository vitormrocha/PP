Ex 51: #include <stdio.h>
#include <math.h>

int main() {

     float xa, xb,ya,yb, xpot, ypot, dist;
     printf("Informe as coordenadas do ponto x: \n");
     scanf("%f %f", &xa, &xb);
     printf ("Informe as coordenadas do pontoy: \n");
     scanf("%f %f", &ya, &yb);
     xpot= xa - xb;
     ypot= ya - yb;
     dist= sqrt ((xpot*xpot)+(ypot*ypot));


     printf("A distancia dos pontos ate a origem e: %f", dist);



        return 0;
}

Ex 52: #include <stdio.h>
#include <math.h>

int main (){
        float amgA, amgB, amgC, premio, invA, invB, invC, invtot, premioA, premioB, premioC;

        printf("Qual o valor que o amigo A investiu? \n");
        scanf("%f", &amgA);
        printf("Qual o valor que o amigo B investiu? \n");
        scanf("%f", &amgB);
        printf ("Qual o valor que o amigo C investiu? \n");
        scanf ("%f", &amgC);
        printf("Qual o valor do premio? \n");
        scanf ("%f", &premio);

        invtot= amgA + amgB + amgC;
        invA= (amgA * 100)/invtot;
        invB= (amgB * 100)/invtot;
        invC= (amgC * 100)/invtot;
        premioA= premio * invA / 100;
        premioB= premio * invB/100;
        premioC = premio * invC/100;


        printf("O amigo A ira aproveitar de: %.1f porcento do premio.\n", premioA);
        printf("O amigo B ira aproveitar de: %.1f porcento do premio.\n", premioB);
        printf("O amigo C ira aproveitar de: %.1f porcento do premio.\n", premioC);

        return 0;

} 

Ex 53: 
#include <stdio.h>
#include <math.h>

int main (){
        float comp, larg, preco, custo, perim;

        printf("Informe o valor do comprimento: \n");
        scanf("%f", &comp);
        printf("Informe o valor da largura: \n");
        scanf("%f", &larg);
        printf("Informe o preco do metro de tela p: \n");
        scanf("%f", &preco);

        perim= comp + larg;
        custo= preco * perim;

        printf("O custo para cerrar esse terreno com a tela e igual a: R$%.1f", custo);



        return 0;

}



