Ex 43: #include <stdio.h>
#include <math.h>

int main() {
    float valtot, val, desc, parc, com, comp;
    printf("Apresente o valor total: ");
    scanf("%f", &valtot);

    desc= (valtot* 0.10);
    val= valtot - desc;
    parc= valtot/3 ;
    com= val*0.05;
    comp= valtot*0.05;

    printf("\nValor  a ser pago com o desconto de 10% : R$%.2f", val);
    printf("\nValor a ser pago do parcelamento sem juros: R$%.2f", parc);
    printf("\nO valor da comissao: R$%.2f", com);
    printf("\n comissao parcelada: R$%.2f", comp);


    return 0;

}

