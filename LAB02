Ex 14 #include <stdio.h>
#include <math.h>
int main (){
    float Gr, Ra, pi;
    pi = 3.14159;
    printf ("Apresente um valor de angulo em graus\n");
    scanf ("%f", &Gr);
    Ra = Gr * pi/180;
    printf("Esse valor em radianos e igual a: %.5f", Ra);

    return 0;


}

Ex 15 #include <stdio.h>
#include <math.h>
int main (){
    float Gr, Ra, pi;
    pi = 3.14159;
    printf ("Apresente um valor de angulo em radianos\n");
    scanf ("%f", &Ra);
    Gr = Ra * 180/pi;
    printf("Esse valor em graus e igual a: %.5f", Gr);

    return 0;


}



