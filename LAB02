Ex 35: #include <stdio.h>
#include <math.h>

int main() {
    float a, b, hip;
    printf(" Apresente os catetos de um triangulo");
    scanf("%f %f",&a, &b);
    hip = sqrt((a*a)+(b*b));
    printf(" A hipotenusa e igual a: %f", hip);
        return 0;

}

Ex 36: #include <stdio.h>
#include <math.h>

int main() {
    float H, vol, R, pi;
    pi= 3.141592;
    printf(" Apresente a altura e o raio de um cilindro: ");
    scanf("%f %f", &H, &R);
    vol= pi * (R * R) * H;
    printf(" O volume do cilindro e igual a: %.6f", vol);
        return 0;

}

