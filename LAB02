Ex 6
#include <stdio.h>
#include <math.h>
int main (){
    float temp, fhrt;
    printf ("Apresente um valor em celsius\n");
    scanf ("%f", &temp);
    fhrt = (temp * 9/5) + 32;
    printf("Essa temperatura em fahrenheit: %.2f", fhrt);

    return 0;
	
	


}

Ex 7
#include <stdio.h>
#include <math.h>
int main (){
    float tempf, tempc;
    printf ("Apresente um valor em fahrenheit\n");
    scanf ("%f", &tempf);
    tempc = 5 * (tempf - 32)/9;
    printf("Essa temperatura em celsius eh: %.2f", tempc);

    return 0;


}

