Ex 46: #include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main() {
        int num, num1, num2,num3;
        printf("Digite um numero inteiro positivo de tres digitos (de 100 a 999): ");
        scanf("%d", &num);
        if (num > 10) {
        num1= num%10;
        num=num/10;
        num2=num%10;
        num=num/10;
        num3= num%10;
        }
        printf ("\nEsse numero invertido e igual a: %d%d%d", num1, num2, num3);



    return 0;

}

Ex 47: #include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main() {
        int num, num1, num2, num3, num4;
        printf("Digite um numero inteiro de 4 digitos (1000 a 9999): ");
        scanf("%d", &num);
        num1= num%10;
        num2= (num%100)/10;
        num3= (num%1000)/100;
        num4= (num%10000)/1000;

        printf("\n %d", num1);
        printf("\n %d", num2);
        printf("\n %d", num3);
        printf("\n %d", num4);



    return 0;

}

Ex 48: #include <stdio.h>
#include <math.h>
#include <stdlib.h>

int main() {
        int seg, min, hor;
        printf("Informe um numero em segundos");
        scanf("%d", &seg);
        hor= seg / 3600;
        min=(seg-(hor*3600))/60;
        seg= seg- (hor*3600)-(min*60);

        printf("Esse valor em horas, minutos e segundos e: %d %d %d", hor, min, seg);


    return 0;

}
 
 Ex 49: #include <stdio.h>
#include <math.h>

int main() {
        int s1,m1,h1,dur1, hfin, sfin, mfin, hf, mf, sf;
        printf("Digite o horario. Informe os dados do inico da experiencia em horas, minutos, e segundos: ");
        scanf("%d %d %d", &h1, &m1, &s1);
        printf("Qual a duracao da experiencia em segundos: ");
        scanf ("%d", &dur1);
        hfin= dur1/3600;
        mfin= (dur1%3600) / 60;
        sfin= (dur1%3600)%60;
        hf= h1 + hfin;
        mf= mfin + m1;
        sf= sfin + s1;




        printf("\n A experiencia terminara as: %d horas %d minutos %d segundos", hf, mf, sf);

        return 0;
}


